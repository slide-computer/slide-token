type SetCustodianArgs = record {
    custodian: principal;
    approved: bool;
};

type SetCustodiansError = variant {
    NotAllowed;
    MaxCustodians: nat;
    TemporarilyUnavailable;
    GenericError : record {
        error_code : nat;
        message : text
    };
};

service : {
    sld4_get_custodians: () -> (vec principal) query;
    sld4_set_custodian: (SetCustodianArgs) -> (variant {
        Ok: nat;
        Err: SetCustodiansError
    });
}
